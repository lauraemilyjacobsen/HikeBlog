@model FlickrNet.PhotosetPhotoCollection
@{
	Layout = null;
}
@if (Model.Count > 0)
{
	<div id="gallery" class="tn3gallery" style="clear:both;">
		@foreach (FlickrNet.Photo photo in Model)
  {
			<a href="@photo.LargeUrl" title="@photo.Title">
				<img src="@photo.SquareThumbnailUrl" alt="@photo.Title" />
			</a>
  }
	</div>	
}
